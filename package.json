{
  "name": "number-suffix",
  "version": "1.0.1",
  "description": "Turns a number to a fixed string with a metric or abbreviation suffix",
  "main": "distribution/index.js",
  "scripts": {
    "test": "tape tests/**/*.spec.js | tap-spec",
    "build": "babel index.js styles.js constants.js utils.js --presets latest --out-dir distribution",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Motistudio/number-suffix.git"
  },
  "keywords": [
    "number",
    "abbreviation",
    "suffix",
    "metric",
    "integer",
    "float",
    "double",
    "convert",
    "fix",
    "toFixed",
    "fixed"
  ],
  "author": "Moti Shriki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Motistudio/number-suffix/issues"
  },
  "homepage": "https://github.com/Motistudio/number-suffix#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
